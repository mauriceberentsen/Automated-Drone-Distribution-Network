@startuml
autonumber
participant ":RosEngineConnector" as REC
participant "msg:Location" as msg
participant "droneEngine:DroneEngine" as DE

[-> REC : setGoal(const float latitude,\nconst float longitude,\nconst float height)
activate REC
create msg
REC -> msg : <<create>>\nlocation(latitude,\nlongitude,height)
REC --> DE -: publish(msg)
@enduml